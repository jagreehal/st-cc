{
  "name": "st-cc",
  "version": "0.7.0",
  "description": "Stencil create component cli",
  "preferGlobal": true,
  "files": [
    "/dist"
  ],
  "bin": {
    "st-cc": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production tsc",
    "dev": "npm run build && concurrently -k \"npm run watch\" \"npm run nodemon\"",
    "nodemon": "nodemon --inspect=0.0.0.0:9230  --watch dist --ignore **/*.spec.js dist/index.js",
    "test": "jest",
    "test-watch": "jest --watch",
    "tsc-watch": "tsc -w"
  },
  "author": "Jag Reehal",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.3.2",
    "commander": "^2.15.1",
    "fs-extra": "^6.0.1",
    "inquirer": "^6.0.0",
    "source-map-support": "^0.5.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.1",
    "@types/inquirer": "^0.0.42",
    "@types/jest": "^23.1.5",
    "@types/node": "^10.5.2",
    "@types/prettier": "^1.10.0",
    "@types/source-map-support": "^0.4.0",
    "concurrently": "3.6.0",
    "cross-env": "5.2.0",
    "jest": "^23.4.0",
    "nodemon": "^1.17.3",
    "prettier": "^1.11.1",
    "ts-jest": "23.0.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "5.9.2",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^2.8.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/*.spec.(ts|tsx)"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "node_modules/",
      "dist"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "dist",
      "**/*.spec.(ts|tsx)"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ]
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jagreehal/st-cc.git"
  },
  "bugs": {
    "url": "https://github.com/jagreehal/st-cc/issues"
  },
  "homepage": "https://github.com/jagreehal/st-cc#readme"
}
